set showmode
set tabstop=8 softtabstop=0 shiftwidth=8
autocmd FileType python setlocal tabstop=8 softtabstop=0 expandtab shiftwidth=4 smarttab
set autoindent
set smartindent
set cindent
syntax enable
set pastetoggle=<F5>
noremap  <Up> ""
noremap! <Up> <Esc>
noremap  <Down> ""
noremap! <Down> <Esc>
noremap  <Left> ""
noremap! <Left> <Esc>
noremap  <Right> ""
noremap! <Right> <Esc>
